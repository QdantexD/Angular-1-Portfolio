{
  "name": "angular-personalization-app",
  "version": "2.1.0",
  "description": "Aplicación Angular avanzada con sistema de personalización completo - Desarrollado por Eddi Andreé Salazar Matos",
  "author": "Eddi Andreé Salazar Matos",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:gh-pages": "ng build --configuration production --base-href=/Angular-1-Portfolio/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
